@page
@model garden.Pages.Products.PestcontrolModel
@{
    ViewData["Title"] = "Pest-Control";
}
<!DOCTYPE html>
<html>
<head>
    <title>Pest Control Shop</title>
    <link rel="stylesheet" href="~/css/products.css" asp-append-version="true" />
</head>
<body>
    <div class="product-card">
        <div class="card">
            <img src="pest1.jpg" alt="Pest Control 1">
            <h5 class="card-title">Pest Control Option 1</h5>
            <p class="card-price">€500-€900</p>
            <button class="add-to-cart" data-name="Pest Control Option 1" data-price="500-900">Add to Cart</button>
        </div>
        <div class="card">
            <img src="pest2.jpg" alt="Pest Control 2">
            <h5 class="card-title">Pest Control Option 2</h5>
            <p class="card-price">€500-€900</p>
            <button class="add-to-cart" data-name="Pest Control Option 2" data-price="500-900">Add to Cart</button>
        </div>
        <div class="card">
            <img src="pest3.jpg" alt="Pest Control 3">
            <h5 class="card-title">Pest Control Option 3</h5>
            <p class="card-price">€500-€900</p>
            <button class="add-to-cart" data-name="Pest Control Option 3" data-price="500-900">Add to Cart</button>
        </div>
    </div>

 
</body>
</html>
